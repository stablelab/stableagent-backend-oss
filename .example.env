########################################
# Non-secret environment configuration #
########################################

# Database Configuration (non-secret)
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=postgres
DATABASE_USER=postgres

# App behavior
ALLOWED_ORIGINS=.*
ENV_TYPE=development

# LLM Provider & Model Defaults (non-secret)
# Available providers: vertex_ai, gemini, openai
DEFAULT_LLM_PROVIDER=gemini
VERTEX_DEFAULT_MODEL=gemini-3-flash-preview
OPENAI_DEFAULT_MODEL=gpt-4o-mini
EMBEDDING_MODEL_NAME=gemini-embedding-001

# LangGraph controls
# Enable LangGraph (default enabled in code paths)
# USE_LANGGRAPH=1
# Select checkpointer backend: memory (default) or sqlite
# LANGGRAPH_CHECKPOINTER=sqlite
# LANGGRAPH_SQLITE_PATH=./data/langgraph.sqlite
# Cap iterations for single-agent and duo-agent graphs
# LANGGRAPH_MAX_ITERS=12
# LANGGRAPH_DUO_MAX_ITERS=16
# Select agent mode: single (default) or duo
# Trio agent (planner + analyst + reasoner + think) is the default
# LANGGRAPH_MODE=trio
# Optional distinct models for duo mode
# REASONER_MODEL=gemini-3-pro-preview
# EXECUTOR_MODEL=gemini-3-flash-preview
# Optional models for trio mode
# AGGREGATOR_MODEL=gemini-3-flash-preview
# THINK_MODEL=gemini-3-pro-preview

# Optional: Vertex AI location/project overrides (if not using src.config.common_settings constants)
# GOOGLE_CLOUD_PROJECT=your_gcp_project_id
# GCLOUD_PROJECT=your_gcp_project_id
# GOOGLE_CLOUD_REGION=us-central1
# VERTEX_LOCATION=us-central1

# ThirdWeb public client config (non-secret)
THIRDWEB_AUTH_CLIENT_ID=0x0000000000000000000000000000000000000000
THIRDWEB_AUTH_DOMAIN=http://localhost:3000


############################
# Secrets (DO NOT COMMIT)  #
############################

# Database secret
DATABASE_PASSWORD=your_database_password_here

# StableLab auth token
STABLELAB_TOKEN=your_stablelab_token_here

# OpenAI API key
OPENAI_API_KEY=your_openai_api_key_here

# (Legacy) Redis settings no longer used
# REDIS_HOST=your_redis_host_here
# REDIS_PORT=6379

# Web search providers (optional)
# Tavily Search API Key
TAVILY_API_KEY=your_tavily_api_key_here
# Exa API Key
EXA_API_KEY=your_exa_api_key_here
# Google Search is done via built in API Key
# Cohere API Key (for reranking)
COHERE_API_KEY=your_cohere_api_key_here

# E2B Code Execution (for code_execute tool)
E2B_API_KEY=your_e2b_api_key_here
CODE_EXECUTION_MODEL=claude-4-5-sonnet-latest